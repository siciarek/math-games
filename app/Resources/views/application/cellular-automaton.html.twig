{% extends 'cdisplay.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    <script src="js/cellular-automaton.js"></script>
    <script>
        var speed = 300;
        var app = null;
        var display = null;

        $(document).ready(function () {
            $('#display').addClass('ca');
            app = new CellularAutomaton(31, 16);

            display = new Display(app, 6);
            display.run(speed);
        });
    </script>
{% endblock %}