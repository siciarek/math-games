{% extends 'board.html.twig' %}

{% block javascripts %}
    {{ parent() }}
    <script src="/js/game-of-life-patterns.js"></script>
    <script src="/js/game-of-life.js"></script>
    <script>
        var speed = 150;
        var app = null;
        var board = null;

        $(document).ready(function () {
            board = new Board();

            app = new GameOfLife(board);

            run(app, speed);
        });
    </script>
{% endblock %}